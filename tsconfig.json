{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "module": "commonjs",
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "declaration": true,
    "outDir": "./dist"
  },
  "exclude": ["node_modules"]
}
